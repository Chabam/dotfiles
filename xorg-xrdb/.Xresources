rofi.color-enabled: true
rofi.color-window: back, base07,  back
rofi.color-normal: back, base07,  back, base07,  back
rofi.color-active: back, base0A,  back, base0A,  back
rofi.color-urgent: back, base0B,  back, base0B,  back

URXVT*scrollTtyOutput: false
URXVT*scrollWithBuffer: true
URXT*scrollTtyKeypress: true

URxvt.font: xft: Gohu GohuFont:size=11

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.internalBorder: 15

URxvt.scrollStyle:        rxvt
URxvt.scrollBar:          false

